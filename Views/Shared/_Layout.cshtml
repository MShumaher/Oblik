<!DOCTYPE HTML>
<html>
<head>
    @await Html.PartialAsync("MetatagsPartial")
    @await Html.PartialAsync("CssPartial")

    
</head>
<body class="is-preload">
    <div id="page-wrapper">

        @await Html.PartialAsync("HeaderPartial")
        
        @RenderBody()
        @await Html.PartialAsync("HighlightsPartial")


        <!-- Gigantic Heading -->
        <section class="wrapper style2">
            <div class="container">
                <header class="major">
                    <h2>A gigantic heading you can use for whatever</h2>
                    <p>With a much smaller subtitle hanging out just below it</p>
                </header>
            </div>
        </section>

        

        <!-- CTA -->
        <section id="cta" class="wrapper style3">
            <div class="container">
                <header>
                    <h2>Are you ready to continue your quest?</h2>
                    <a href="#" class="button">Insert Coin</a>
                </header>
            </div>
        </section>

        @await Html.PartialAsync("FooterPartial")
    </div>

    @await Html.PartialAsync("ScriptsPartial")
    
</body>
</html>