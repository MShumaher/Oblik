@model IQueryable<Visit>


@if (Model.Any())
{
    foreach (Visit entity in Model)
    {
        <section class="wrapper style1">
            <div class="container">
                <div class="row">
                    <section class="col-6 col-12-narrower">
                        <div class="box post">
                            <a asp-area="" asp-controller="VisitServices" asp-action="Index" asp-route-id="@entity.VisitId">
                                <div class="inner">
                                    <h3>@entity.PatientId</h3>
                                    <p>@entity.DoctorId @entity.DoctorId</p>
                                </div>
                            </a>
                        </div>
                    </section>
                </div>

            </div>
        </section>
    }
}